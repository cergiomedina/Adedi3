<h1>Listing parametro_sistemas</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @parametro_sistemas.each do |parametro_sistema| %>
      <tr>
        <td><%= link_to 'Show', parametro_sistema %></td>
        <td><%= link_to 'Edit', edit_parametro_sistema_path(parametro_sistema) %></td>
        <td><%= link_to 'Destroy', parametro_sistema, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Parametro sistema', new_parametro_sistema_path %>
