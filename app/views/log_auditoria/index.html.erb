<h1>Listing log_auditoria</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @log_auditoria.each do |log_auditorium| %>
      <tr>
        <td><%= link_to 'Show', log_auditorium %></td>
        <td><%= link_to 'Edit', edit_log_auditorium_path(log_auditorium) %></td>
        <td><%= link_to 'Destroy', log_auditorium, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Log auditorium', new_log_auditorium_path %>
