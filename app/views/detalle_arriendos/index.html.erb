<h1>Listing detalle_arriendos</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @detalle_arriendos.each do |detalle_arriendo| %>
      <tr>
        <td><%= link_to 'Show', detalle_arriendo %></td>
        <td><%= link_to 'Edit', edit_detalle_arriendo_path(detalle_arriendo) %></td>
        <td><%= link_to 'Destroy', detalle_arriendo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Detalle arriendo', new_detalle_arriendo_path %>
