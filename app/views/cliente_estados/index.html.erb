<h1>Listing cliente_estados</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cliente_estados.each do |cliente_estado| %>
      <tr>
        <td><%= link_to 'Show', cliente_estado %></td>
        <td><%= link_to 'Edit', edit_cliente_estado_path(cliente_estado) %></td>
        <td><%= link_to 'Destroy', cliente_estado, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

