<h1>Listing ejemplars</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ejemplars.each do |ejemplar| %>
      <tr>
        <td><%= link_to 'Show', ejemplar %></td>
        <td><%= link_to 'Edit', edit_ejemplar_path(ejemplar) %></td>
        <td><%= link_to 'Destroy', ejemplar, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ejemplar', new_ejemplar_path %>
